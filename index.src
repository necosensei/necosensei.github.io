\[

    %  共役勾配法 # 超収束とかいう謎の現象を起こすらしい # 12
    %  https://ja.wikipedia.org/wiki/%E5%85%B1%E5%BD%B9%E5%8B%BE%E9%85%8D%E6%B3%95

    \begin{align}
      &amp;
        \forall \vec{x}_0 \in \R^n, \;
        \exists k \le n : A \vec{x}_{k} = \vec{b},\\
      &amp;\graysmall{
        \begin{align}
          \mbox{where}\;
            &amp; A : \mbox{symmetric positive definite matrix in}\;\R^{n \times n},\;
                \vec{b} \in \R^n, \\
            &amp; \vec{r}_{0} = \vec{p}_{0} = \vec{b} - A \vec{x}_0, \\
            &amp; 
	    \begin{array}{ll}
	      \displaystyle
	      \vec{x}_{k+1} = \vec{x}_{k}   + \frac{ \|\vec{r}_k    \|^2 }{\vec{p}_k \cdot A \vec{p}_k} \vec{p}_k,
              &amp; \displaystyle
	        \vec{r}_{k+1} = \vec{r}_{k}   - \frac{ \|\vec{r}_k    \|^2 }{\vec{p}_k \cdot A \vec{p}_k} A \vec{p}_k,\\
              &amp; \displaystyle
	        \vec{p}_{k+1} = \vec{r}_{k+1} + \frac{ \|\vec{r}_{k+1}\|^2 }{ \|\vec{r}_k\|^2 } \vec{p}_k.   \\
	    \end{array}
        \end{align}
      }
    \end{align}
\]
