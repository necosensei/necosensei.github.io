#! /usr/local/bin/perl | ./svg-turtle.pl | /home/hoshino/.npm-global/bin/svgo -i - -o -

$N=3;

$pi  = 3.14159_26535_89793_23846_26433_83279_50288 ;
$cos = cos($pi/3.0) ;
$sin = sin($pi/3.0) ;

sub operate{
  my($n,$x,$y,$dx,$dy) = @_ ;
  my($dx0,$dy0,$dx1,$dy1,$dx2,$dy2);
  if($n < $N){
    $n++ ;
    $dx0 =   $dx/3.0 ;
    $dy0 =   $dy/3.0 ;
    $dx1 =   $cos * $dx0 - $sin * $dy0 ;
    $dy1 =   $sin * $dx0 + $cos * $dy0 ;
    $dx2 =   $cos * $dx0 + $sin * $dy0 ;
    $dy2 = - $sin * $dx0 + $cos * $dy0 ;

    &operate($n,$x,$y,$dx0,$dy0) ;
    $x += $dx0 ;
    $y += $dy0 ;
    &operate($n,$x,$y,$dx1,$dy1) ;
    $x += $dx1 ;
    $y += $dy1 ;
    &operate($n,$x,$y,$dx2,$dy2) ;
    $x += $dx2 ;
    $y += $dy2 ;
    &operate($n,$x,$y,$dx0,$dy0) ;
  }else{
    print("  \\line $x $y ");
    $x += $dx ;
    $y += $dy ;
    print("$x $y\n");
  }
}

print("\\svg 200 200\n") ;
print("\\transform 200 0 0 -200 0 200 {\n") ;
&operate(0,0,0,1,1) ;
print("}\n") ;

#!

