\[

  % Beatty sequence

  \begin{aligned}
    &amp; \forall r \in \R\setminus\Q \; \st r &gt; 1, \\
    &amp; {\cal B}_r \cap {\cal B}_s = \varnothing \; \wedge \;
          {\cal B}_r \cup {\cal B}_s = \N^+, \\
    &amp; \where {\cal B}_x = \{\lfloor n x \rfloor \mid n \in \N^+ \}
          \text{ and } s = r/(r-1).
  \end{aligned}

\]

