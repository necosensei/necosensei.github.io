\[

   % Zuckerman number

   \begin{aligned}
     &amp; N = \sum_{i \in \{0,\cdots,n-1\}} a_i \, b^i \st \left(\prod_i a_i\right) \backslash \; N, \\
     &amp; \where b \in \N_{>1} \text{ is a radix, and } \; a_i \in \{1,\cdots,b-1\} \text{ for all } i \in \{0,\cdots,n-1\}.
   \end{aligned}

\]

