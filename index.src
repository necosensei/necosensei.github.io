    % Cantor function # 172

    \begin{align}
      &amp; f_n|_{n\rightarrow \infty} : [0,1]  \rightarrow [0,1], \\
      &amp; \black{
        \begin{align}
          \where 
            &amp; f_0(x) = x, \\
            &amp; f_{n+1}(x) = \left\{
              \begin{array}{ll}
                \frac{1}{2} \times f_n(3x)   &amp; \text{if $ 0 \le x &lt; \frac{1}{3} $}, \\
                \frac{1}{2}                  &amp; \text{if $ \frac{1}{3} \le x &lt; \frac{2}{3} $}, \\
                \frac{1}{2} +
                \frac{1}{2} \times f_n(3x-2) &amp; \text{if $ \frac{2}{3} \le x \le 1 $}. \\
              \end{array}
            \right.
        \end{align}
      }
    \end{align}

\]

