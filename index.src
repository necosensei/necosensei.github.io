$pi    = 3.14159_26535_89793_23846_26433_83279_50288 ;

$N=12;

sub name{
  my($alpha) = @_ ;
  return(
    ($alpha+$N*$N) % $N
  ) ;
}

print("graph FranklinGraph{\n") ;
print("  node [shape=point];\n") ;
print("  edge [color=black];\n") ;

for($alpha=0; $alpha<$N; $alpha++){
    $a = &name($alpha) ;
    $b = &name($alpha+1) ;
    $c = &name($alpha-1) ;
    $d = &name($alpha+(1-($alpha % 2)*2)*5) ;

    if($a < $b){$A{"$a,$b"}=1;}else{$A{"$b,$a"}=1;}
    if($a < $c){$A{"$a,$c"}=1;}else{$A{"$c,$a"}=1;}
    if($a < $d){$A{"$a,$d"}=1;}else{$A{"$d,$a"}=1;}

    $r = 1 ;
    $theta  = (2*$pi*$a/($N*1.0));
    $x = $r * cos($theta) ;
    $y = $r * sin($theta) ;
    print("  $a [pos=\"$x,$y!\"];\n") ;
}

for $i (keys(%A)){
  ($a,$b) = split(/,/,$i,2) ;
  print("  ${a}--${b}; \n") ;
}
print("}\n") ;
