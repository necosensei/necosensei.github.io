#! /usr/local/bin/perl | ./svg-turtle.pl | /home/hoshino/.npm-global/bin/svgo -i - -o -

$b0 = 0.4 ;
$b1 = 0.6 ;
$b2 = ($b0*$b0+$b1*$b1) ;

$a0 = 1.0/sqrt($b0*$b0+$b1*$b1+1) ;
$a1 = 1.0/sqrt($b0*$b0+$b1*$b1) ;
$a2 = 1.0/sqrt($b0*$b0+$b1*$b1+$b2*$b2) ;

@Z = (   1*$a0, $b0*$a0, $b1*$a0) ;
@X = (   0*$a1, $b1*$a1,-$b0*$a1) ;
@Y = (-$b2*$a2, $b0*$a2, $b1*$a2) ;

sub inner_product{
  my($x1,$y1,$z1,$x2,$y2,$z2) = @_ ;
  return $x1*$x2 +$y1*$y2 +$z1*$z2 ;
}

sub projection{
  return (&inner_product(@_,@X), &inner_product(@_,@Y)) ;
}


sub plot{
  my($x,$y) = &projection(@_);
  print <<ENDS ;
\\circle $x $y 0.05
ENDS
}

sub rect{
  my(
    $x0,$y0,$z0,
    $x1,$y1,$z1,
    $x2,$y2,$z2,
    $x3,$y3,$z3
  ) = @_ ;
  my($X0,$Y0) = &projection($x0,$y0,$z0);
  my($X1,$Y1) = &projection($x1,$y1,$z1);
  my($X2,$Y2) = &projection($x2,$y2,$z2);
  my($X3,$Y3) = &projection($x3,$y3,$z3);
  print <<ENDS ;
  \\path M $X0 $Y0 L $X1 $Y1 L $X2 $Y2 L $X3 $Y3 Z
ENDS

}

